<ul>
    <li><a href="../welcome/index">Home</a></li>
    
    <li><a href="articles/new">Create Article</a></li>
    <li><%= link_to 'Article List', articles_path %></li>
</ul>


<h1> New article</h1>



<%= form_for :article, url: articles_path  do |f| %>  
<p>     
   <%= f.label :title %><br>     <%= f.text_field :title %>   
</p>   

<p>     
   <%= f.label :text %><br>     <%= f.text_area :text %>   
</p>  

<p> 
   <%= f.submit %>  
</p> 

<% end %>
<%= link_to 'Back', articles_path %>

